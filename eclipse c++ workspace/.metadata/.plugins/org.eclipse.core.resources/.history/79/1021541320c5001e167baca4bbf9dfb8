#include <iostream>
#include <string>
using namespace std;

int main() {
    int n;
    cin >> n;

    int x = n/2;

    for (int i = 1; i < x; i += 2) {
        cout << " ";
        for (int j = 0; j < i; j++) {
                    cout << "*";
        }
        cout << "\n";
    }


    return 0;
}
