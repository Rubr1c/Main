#include <iostream>
#include <string>
using namespace std;

int main() {
    int n;
    cin >> n;


    int j = 0;

    for (int i = 0; i < n; i+= 2) {
        cout << " ";
        do {
        	j++;
        	cout << "*";
        }
        while(j < i);
        cout << "\n";
    }


    return 0;
}
